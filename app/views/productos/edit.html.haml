#page-content
  %h1.page-title
    = page_title! 'Editando producto'

  .content.presentacion
    = image_tag @producto.imagen_url(:vertical) if @producto.imagen?

  .content
    = render 'form'

    = link_to 'Regresar', reino_categoria_subcategoria_tipo_productos_path(@reino, @categoria, @subcategoria, @tipo), class: 'button-back-cms'
